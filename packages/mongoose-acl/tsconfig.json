{
  "compilerOptions": {
    "target": "ES2015",
    "rootDir": "src",
    "outDir": "lib",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false
  },
  "include": ["src"]
}
